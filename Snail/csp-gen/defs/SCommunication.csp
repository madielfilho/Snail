	MemoryVariablesSyncSet = Union({
		{|
			set_updated,
			set_location,
			getV_updated,
			get__.TID_SCommunication_t2,
			getV_got,
			getV_neighs,
			set_neighs,
			coordinatesFiware__.TID_SCommunication_SGetNeighs_t1,
			update__.TID_SCommunication_t7,
			set_got,
			getV_location
		|}	
	})
	
	MemoryVariablesHideSet = Union({
		{|
			getV_updated,
			getV_got,
			getV_neighs,
			getV_location
		|}
	})
	
	-- combined individual memory processes
	MemoryVariables = ||| (P, alpha) : MemoryVariablesProcesses_SCommunication @ P
	
	-- complete memory process	
	MemoryN(id__, const_SCommunication_max, const_SCommunication_timeout) = sbisim(diamond((sbisim(diamond(MemoryVariables)) [| MemoryVariablesSyncSet |] sbisim(diamond(MemoryTransitions_SCommunication(id__, const_SCommunication_max, const_SCommunication_timeout)))) \ MemoryVariablesHideSet))		
	
	-- main process
	
	MachineMemorySyncSet = Union({
		union(
			union(
				{|get_neighs,set_neighs,
				get_location,set_location,
				get_updated,set_updated,
				get_got,set_got|},
				{||}
			)
				
			,
			{|internal__.TID_SCommunication_SGetNeighs_t0,
			coordinatesFiware__.TID_SCommunication_SGetNeighs_t1,
			internal__.TID_SCommunication_SUpdate_t0,
			internal__.TID_SCommunication_SUpdate_t1,
			internal__.TID_SCommunication_t0,
			connect__.TID_SCommunication_t1,
			get__.TID_SCommunication_t2,
			finished__.TID_SCommunication_t6,
			update__.TID_SCommunication_t7,
			internal__.TID_SCommunication_t8,
			internal__.TID_SCommunication_t3,
			internal__.TID_SCommunication_t4,
			internal__.TID_SCommunication_t5,
			internal__.TID_SCommunication_t9,
			internal__.TID_SCommunication_t10|}
		)
	})
	
	MachineMemoryHidingSet = Union({
		union(
			{|get_neighs,set_neighs,
			get_location,set_location,
			get_updated,set_updated,
			get_got,set_got|},
			{||}
		)
	})
	
	MachineInternalEvents = {|
		internal__
	|}
	
	AUX(id__,
			    const_SCommunication_max,
			    const_SCommunication_timeout) = (
		(
			STM(id__,
					    const_SCommunication_max,
					    const_SCommunication_timeout)
				[|MachineMemorySyncSet|]
			sbisim(diamond(MemoryN(id__, const_SCommunication_max, const_SCommunication_timeout)))
		)[[
			updateLoc__.x____ <- updateLoc,
			get__.x____ <- get,
			connect__.x____ <- connect,
			connect_ack__.x____ <- connect_ack,
			coordinatesReq__.x____ <- coordinatesReq,
			coordinates__.x____ <- coordinates,
			coordinatesFiware__.x____ <- coordinatesFiware,
			update__.x____ <- update,
			updateReq__.x____ <- updateReq,
			updateFiware__.x____ <- updateFiware,
			finished__.x____ <- finished,
			communicationError__.x____ <- communicationError
			| x____ <- TIDS
		]]
		\MachineMemoryHidingSet
	)
	[|{|terminate|}|>SKIP\MachineInternalEvents		

	-- definition of the behaviour of state machines (default, optimised, visible, visible and optimised)
	transparent diamond
	transparent sbisim
	transparent chase
			
	FVS__(id__,
			    const_SCommunication_max,
			    const_SCommunication_timeout) = AUX(id__,
			    const_SCommunication_max,
			    const_SCommunication_timeout)
	D__(id__,
			    const_SCommunication_max,
			    const_SCommunication_timeout) = AUX(id__,
			    const_SCommunication_max,
			    const_SCommunication_timeout) \ internal_events
	O__(id__,
			    const_SCommunication_max,
			    const_SCommunication_timeout) = sbisim(diamond(D__(id__,
			    const_SCommunication_max,
			    const_SCommunication_timeout)))
	VS__(id__,
			    const_SCommunication_max,
			    const_SCommunication_timeout) = ShowV(FVS__(id__,
			    const_SCommunication_max,
			    const_SCommunication_timeout))
	VS_O__(id__,
			    const_SCommunication_max,
			    const_SCommunication_timeout) = sbisim(diamond(VS__(id__,
			    const_SCommunication_max,
			    const_SCommunication_timeout)))
	HEXT__(id__,
			    const_SCommunication_max,
			    const_SCommunication_timeout) = O__(id__,
			    const_SCommunication_max,
			    const_SCommunication_timeout) [|shared_variable_events|] SKIP	
endmodule
